CREATE TABLE stock_finance_income(
`ts_code` varchar(20) not null default '' comment 'TS股票代码',
`ann_date` int(20) not null default 0 comment '公告日期',
`f_ann_date` int(20) not null default 0 comment '实际公告日期，即发生过数据变更的最终日期',
`end_date` int(20) not null default 0 comment '报告期',
`report_type` varchar(20) not null default '' comment '报告类型： 参考下表说明',
`comp_type` varchar(20) not null default '' comment '公司类型：1一般工商业 2银行 3保险 4证券',
`basic_eps` float(24) comment '基本每股收益',
`diluted_eps` float(24) comment '稀释每股收益',
`total_revenue` float(24) comment '营业总收入 (元，下同)',
`revenue` float(24) comment '营业收入',
`int_income` float(24) comment '利息收入',
`prem_earned` float(24) comment '已赚保费',
`comm_income` float(24) comment '手续费及佣金收入',
`n_commis_income` float(24) comment '手续费及佣金净收入',
`n_oth_income` float(24) comment '其他经营净收益',
`n_oth_b_income` float(24) comment '加:其他业务净收益',
`prem_income` float(24) comment '保险业务收入',
`out_prem` float(24) comment '减:分出保费',
`une_prem_reser` float(24) comment '提取未到期责任准备金',
`reins_income` float(24) comment '其中:分保费收入',
`n_sec_tb_income` float(24) comment '代理买卖证券业务净收入',
`n_sec_uw_income` float(24) comment '证券承销业务净收入',
`n_asset_mg_income` float(24) comment '受托客户资产管理业务净收入',
`oth_b_income` float(24) comment '其他业务收入',
`fv_value_chg_gain` float(24) comment '加:公允价值变动净收益',
`invest_income` float(24) comment '加:投资净收益',
`ass_invest_income` float(24) comment '其中:对联营企业和合营企业的投资收益',
`forex_gain` float(24) comment '加:汇兑净收益',
`total_cogs` float(24) comment '营业总成本',
`oper_cost` float(24) comment '减:营业成本',
`int_exp` float(24) comment '减:利息支出',
`comm_exp` float(24) comment '减:手续费及佣金支出',
`biz_tax_surchg` float(24) comment '减:营业税金及附加',
`sell_exp` float(24) comment '减:销售费用',
`admin_exp` float(24) comment '减:管理费用',
`fin_exp` float(24) comment '减:财务费用',
`assets_impair_loss` float(24) comment '减:资产减值损失',
`prem_refund` float(24) comment '退保金',
`compens_payout` float(24) comment '赔付总支出',
`reser_insur_liab` float(24) comment '提取保险责任准备金',
`div_payt` float(24) comment '保户红利支出',
`reins_exp` float(24) comment '分保费用',
`oper_exp` float(24) comment '营业支出',
`compens_payout_refu` float(24) comment '减:摊回赔付支出',
`insur_reser_refu` float(24) comment '减:摊回保险责任准备金',
`reins_cost_refund` float(24) comment '减:摊回分保费用',
`other_bus_cost` float(24) comment '其他业务成本',
`operate_profit` float(24) comment '营业利润',
`non_oper_income` float(24) comment '加:营业外收入',
`non_oper_exp` float(24) comment '减:营业外支出',
`nca_disploss` float(24) comment '其中:减:非流动资产处置净损失',
`total_profit` float(24) comment '利润总额',
`income_tax` float(24) comment '所得税费用',
`n_income` float(24) comment '净利润(含少数股东损益)',
`n_income_attr_p` float(24) comment '净利润(不含少数股东损益)',
`minority_gain` float(24) comment '少数股东损益',
`oth_compr_income` float(24) comment '其他综合收益',
`t_compr_income` float(24) comment '综合收益总额',
`compr_inc_attr_p` float(24) comment '归属于母公司(或股东)的综合收益总额',
`compr_inc_attr_m_s` float(24) comment '归属于少数股东的综合收益总额',
`ebit` float(24) comment '息税前利润',
`ebitda` float(24) comment '息税折旧摊销前利润',
`insurance_exp` float(24) comment '保险业务支出',
`undist_profit` float(24) comment '年初未分配利润',
`distable_profit` float(24) comment '可分配利润',
key (ts_code, end_date))
PARTITION BY RANGE(end_date)
(
PARTITION p2016 VALUES LESS THAN (20170101),
PARTITION p2017 VALUES LESS THAN (20180101),
PARTITION p2018 VALUES LESS THAN (20190101),
PARTITION p2019 VALUES LESS THAN (20200101),
PARTITION p2020 VALUES LESS THAN (20210101),
PARTITION p2021 VALUES LESS THAN (20220101),
PARTITION p2022 VALUES LESS THAN (20230101)
);
